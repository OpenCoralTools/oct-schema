oct_schema:Coral a sh:NodeShape ;
    sh:closed true ;
    sh:description "Represents a specific coral instance" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "ID of the user who created the record" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path oct_schema:created_by_id ],
        [ sh:datatype xsd:string ;
            sh:description "ID of the genet" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path oct_schema:genet_id ],
        [ sh:datatype xsd:string ;
            sh:description "ID of the user who last updated the record" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path oct_schema:updated_by_id ],
        [ sh:datatype xsd:string ;
            sh:description "Name of the entity" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path oct_schema:name ],
        [ sh:datatype xsd:integer ;
            sh:description "Quantity of items" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path oct_schema:quantity ],
        [ sh:datatype xsd:string ;
            sh:description "ID of the group/location" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path oct_schema:group_id ],
        [ sh:datatype xsd:string ;
            sh:description "ID of the organization this record belongs to" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path oct_schema:organization_id ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Timestamp when the record was last updated" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path oct_schema:updated_at ],
        [ sh:datatype xsd:string ;
            sh:description "URL friendly path to this resource" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path oct_schema:url_path ],
        [ sh:datatype xsd:string ;
            sh:description "Internal path structure" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path oct_schema:internal_path ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Unique identifier for the record" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path oct_schema:id ],
        [ sh:description "Size of the coral" ;
            sh:in ( "xs" "s" "m" "l" "xl" ) ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path oct_schema:coral_size ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Timestamp when the record was created" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path oct_schema:created_at ],
        [ sh:datatype xsd:string ;
            sh:description "ID of the site this entity belongs to" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path oct_schema:site_id ],
        [ sh:datatype xsd:string ;
            sh:description "ID of the species" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path oct_schema:species_id ],
        [ sh:datatype xsd:string ;
            sh:description "URL friendly identifier part" ;
            sh:maxCount 1 ;
