      "slot_uri": "https://opencoral.tools/oct-schema/coral/genet/speciesCode",
      "alias": "speciesCode",
      "owner": "CoralTaxonRef",
      "domain_of": [
        "CoralTaxonRef"
      ],
      "range": "string",
      "required": true,
      "pattern": "^[a-z0-9]{4}$",
